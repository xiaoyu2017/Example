<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="categoryMapper">
    <insert id="add" parameterType="cn.fishland.bookmanager.bean.pojo.Category">
        insert into category(`name`, `sort`, `status`) value (#{name}, #{sort}, #{status});
    </insert>

    <update id="updateCategory" parameterType="cn.fishland.bookmanager.bean.pojo.Category">
    </update>
</mapper>